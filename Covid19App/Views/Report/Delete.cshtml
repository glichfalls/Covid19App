@model Covid19App.Models.Report

@{
    ViewData["Title"] = "Delete";
}

<h3>Bericht Löschen</h3>

<p>Sind Sie sicher, dass Sie die Abteilung löschen wollen?</p>

<div>
    
    <hr/>
    
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Date)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Date)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="submit" value="Löschen" class="btn btn-primary"/>
        <a class="btn btn-danger" asp-action="Index">Abbrechen</a>
    </form>
    
</div>
